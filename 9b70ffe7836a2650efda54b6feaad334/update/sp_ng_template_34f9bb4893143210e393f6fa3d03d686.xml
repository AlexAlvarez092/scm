        <template><![CDATA[<!-- Card colors -->
<!-- 
    - Navy 
    - Blue
    - Purple
    - Turquoise
    - Apple
    - Petrol
    - Iris
    - Pink
    - Orchide
    - Green
    - Orange
    - Red
    - No color
-->

<!-- Tree item -->
<div class="card card--border card--rounded card--compact card--notification card--{{node.color}} margin-bottom-3">
	<h1 class="card__header">{{node.title}}</h1>
	<div class="card__body">
		<div class="card-content">
			<span>
				<strong>ID:</strong>
				{{node.id}}
			</span>
			<span>
				<strong>Parent:</strong>
				{{node.parent || 'Root'}}
			</span>
			<span>
				<strong>Color:</strong>
				{{node.color}}
			</span>
		</div>
		<div class="card-actions">
			<sdx-button
				theme="transparent"
				icon-size="2"
				icon-name="icon-eye"
				ng-click="c.executeCallback('openFlyover', node)"
			></sdx-button>
		</div>
	</div>
</div>
]]></template>
    </sp_ng_template>
</record_update>
