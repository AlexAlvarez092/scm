        <template><![CDATA[<div class="tree-node">
	<ng-include src="'scm-tree-card.html'"></ng-include>
	
	<!-- Recursive children -->
	<div class="tree-children" ng-if="c.hasChildren(node.id)">
		<div
			ng-repeat="child in c.getChildren(node.id) track by child.id"
			ng-init="node = child"
			ng-include="'scm-tree-node-recursive.html'"
		></div>
	</div>
</div>]]></template>
